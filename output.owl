<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.org/onto#"
     xml:base="http://www.example.org/onto"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://www.example.org/onto"/>
    
    <owl:Class rdf:about="http://www.example.org/onto#Animal"/>
    <owl:Class rdf:about="http://www.example.org/onto#Mammal">
        <rdfs:subClassOf rdf:resource="http://www.example.org/onto#Animal"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.example.org/onto#Pet">
        <rdfs:subClassOf rdf:resource="http://www.example.org/onto#Animal"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.example.org/onto#Dog">
        <rdfs:subClassOf rdf:resource="http://www.example.org/onto#Mammal"/>
        <rdfs:subClassOf rdf:resource="http://www.example.org/onto#Pet"/>
    </owl:Class>
</rdf:RDF>
