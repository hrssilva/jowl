<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Ontology rdf:about="http://example.org/"/>

  <owl:Class rdf:about="http://example.org/Person"/>
  <owl:Class rdf:about="http://example.org/Cat"/>
  <owl:Class rdf:about="http://example.org/Dog"/>

  <owl:ObjectProperty rdf:about="http://example.org/hasPet"/>
  
  <owl:Class rdf:about="http://example.org/CatLover">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://example.org/hasPet"/>
        <owl:someValuesFrom rdf:resource="http://example.org/Cat"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/DogLover">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://example.org/hasPet"/>
        <owl:allValuesFrom rdf:resource="http://example.org/Dog"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:NamedIndividual rdf:about="http://example.org/alice">
    <rdf:type rdf:resource="http://example.org/Person"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://example.org/bob">
    <rdf:type rdf:resource="http://example.org/CatLover"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://example.org/charlie">
    <rdf:type rdf:resource="http://example.org/DogLover"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://example.org/whiskers">
    <rdf:type rdf:resource="http://example.org/Cat"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://example.org/fido">
    <rdf:type rdf:resource="http://example.org/Dog"/>
  </owl:NamedIndividual>

  <rdf:Description rdf:about="http://example.org/bob">
    <hasPet rdf:resource="http://example.org/whiskers"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/charlie">
    <hasPet rdf:resource="http://example.org/fido"/>
  </rdf:Description>
</rdf:RDF>
