prefix rr: <http://www.w3.org/ns/r2rml#>
prefix base: <http://totalenergies.fr/data/prenoms/metiers-competencies#>
prefix tbox: <http://totalenergies.fr/data/prenoms/metiers-competencies/tbox#>
prefix abox: <http://totalenergies.fr/data/prenoms/metiers-competencies/abox#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix dc: <http://purl.org/dc/elements/1.1/>
prefix dcterms: <http://purl.org/dc/terms/>
prefix bfo: <http://purl.obolibrary.org/obo/>
prefix ido: <http://rds.posccaesar.org/ontology/lis14/ont/core>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT ?name ?role ?gainTime WHERE {
    ?name a tbox:Prenom ; 
        ido:isAbout ?person .
    ?person a ido:Person ; 
        ido:hasRole ?role ;
        ido:participateIn ?gain .
    ?gain a tbox:GainOfRole ;
        ido:startTimestamp ?gainTime .
}